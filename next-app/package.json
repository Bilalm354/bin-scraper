{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "main": "tsx --no-warnings main.ts",
    "db:push": "npx drizzle-kit push",
    "db:push:prod": "npx drizzle-kit push --config=drizzle-prod.config.ts",
    "db:start": "docker run --name dev-db -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=my_dev_db -p 5432:5432 -d postgres",
    "db:start:persistent": "docker run --name dev-db -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=my_dev_db -p 5432:5432 -v pgdata:/var/lib/postgresql/data -d postgres",
    "db:stop": "docker stop dev-db && docker rm dev-db",
    "db:logs": "docker logs -f dev-db",
    "db:restart": "npm run db:stop && npm run db:start",
    "db:cli:local": "psql postgres://postgres:postgres@localhost:5432/my_dev_db",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:debug": "npx playwright test --debug"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@types/jest": "^29.5.14",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "discord.js": "^14.18.0",
    "drizzle-orm": "^0.39.3",
    "lucide-react": "^0.476.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-autocomplete": "^2.7.4",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "jest": "^29.7.0",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "zlib-sync": "^0.1.9"
  }
}
